package templates 
import (
    "github.com/bagvendt/chores/internal/models"
)

templ RoutineCard(routine models.Routine) {
    return `<div class="routine-card">
        <img draggable="false" class="routine-image" src="${routine.ImageUrl}" alt="${routine.Name}">
        <div class="routine-title">${routine.Name}</div>
        <div class="progress-bar"></div>
        <div class="progress-text">1</div>
      </div>`
}